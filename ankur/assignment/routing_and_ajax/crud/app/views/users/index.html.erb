<div id="index">
    <h1>List of Users</h1>

    <table>
      <tr>
        <th>Name</th>
        <th>DOB</th>
        <th>Age</th>
        <th>Gender</th>
        <th>Contact No</th>
        <th>Email</th>
        <th></th>
        <th></th>
        <th></th>
      </tr>

    <% @users.each do |user| %>
      <tr>
        <td><%= user.name %></td>
        <td><%= user.dob %></td>
        <td><%= user.age %></td>
        <td><%= user.gender %></td>
        <td><%= user.contact_no %></td>
        <td><%= user.email %></td>
        <td><%= link_to 'Show', user %></td>
        <td><%= link_to 'Update', edit_user_path(user) %></td>
        <td><%= link_to 'Destroy', user, method: :delete, remote: true, :class => 'delete_user' %></td>
      </tr>
    <% end %>
    </table>

    <br />

    <%= link_to 'Create user', new_user_path %>
</div>